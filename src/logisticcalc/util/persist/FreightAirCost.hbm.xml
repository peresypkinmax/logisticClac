<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="test2">

    <class name="Spec" table="SPEC">
       <id name="costid" column="COSTID">
            <generator class="native"/>
        </id>        
        <property name="fromCountry" type="string" column="FROMCOUNTRY"/>
        <property name="fromWeight" type="integer" column="FROMWEIGHT"/>
        <property name="toWeight" type="integer" column="TOWEIGHT"/>
        <property name="cost" type="integer" column="COST"/>
    </class>

</hibernate-mapping>